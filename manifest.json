{
    "name": "Tinder Automation",
    "version": "1.0",
    "description": "A Chrome Extension to help you automate things like swiping right and left on Tinder",
    "browser_action": {
        "default_popup": "popup.html",
        "default title": "Click Here To Automate Tinder"
    },
    "permissions": ["tabs", "https://tinder.com/*"],
    "content_scripts": [
        {
        "matches": [
            "https://tinder.com/*"
        ],
        "css": ["style.css"],
        "js": ["popup.js"]
    }
],
    "manifest_version": 2
}